{
  "name": "code-breaker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@xstate/fsm": "^1.4.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "natural": "^2.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spinkit": "^3.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "npm run build && node ./build/backend/index.js",
    "lint": "eslint ./src/**/**.js ./src/**/**.jsx",
    "build": "react-scripts build && babel ./src/backend -s both --plugins source-map-support --out-dir build/backend && babel ./src/shared -s both --plugins source-map-support --out-dir build/shared",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:app": "./script/start-watch.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard",
      "standard-jsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "babel-plugin-source-map-support": "^2.1.2",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "source-map-support": "^0.5.19"
  }
}
